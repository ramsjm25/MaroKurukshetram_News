{
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    },
    "api/test.js": {
      "maxDuration": 10
    },
    "api/health.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/test",
      "destination": "/api/test"
    },
    {
      "source": "/api/health",
      "destination": "/api/health"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api"
    }
  ]
}